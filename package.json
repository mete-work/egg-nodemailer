{
  "name": "@mete-work/egg-nodemailer",
  "version": "1.0.0",
  "description": "nodemailer plugin for Egg.js.",
  "registry": "",
  "eggPlugin": {
    "name": "nodemailer",
    "dependencies": [
      "ejs"
    ]
  },
  "keywords": [
    "egg",
    "eggPlugin",
    "egg-plugin"
  ],
  "dependencies": {
    "@types/nodemailer": "^6.2.1",
    "juice": "^5.2.0",
    "nodemailer": "^6.3.0"
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "scripts": {
